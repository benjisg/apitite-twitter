						formats : ["json", "xml", "rss", "atom"],
						structure : "/home_timeline.{FORMAT}{PARAMS}",
						params : {
							"count" : {
								type : "integer",
								min : 1,
								max : 200,
								optional : true
							},
							"since_id" : {
								type : "integer",
								optional : true
							},
							"max_id" : {
								type : "integer",
								optional : true
							},
